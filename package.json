{
  "name": "expressdemo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku:prebuild": "rimraf dist",
    "start": "NODE_ENV=staging node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "prestart:staging": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:staging",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "~4.16.1",
    "express-validation": "^3.0.6",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.11.15",
    "morgan": "~1.9.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.17"
  }
}